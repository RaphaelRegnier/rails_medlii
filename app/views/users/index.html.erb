<div class="container text-center">
  <div class="row">
    <div class="col-xs-12">
      <% @users.each do | user | %>
        <ul class = "list-unstyled">
          <li> <a href="/users/<%= user.id %>" > <%= user.first_name %> <%user.last_name.first %> </a> </li>
          <li> <%= user.age %></li>
          <li> <%= user.description %> </li>
          <li> <%=user.location %> </li>
        </ul>
       <% end %>
    </div>
  </div>
</div>


